{
    "bitwise": true,
    "camelcase": false, //deprecated
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true, //deprecated
    "noarg": true,
    "noempty": true, //deprecated
    "nonbsp": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": "func",
    "sub": true, // allow object['field'] access variant
    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": false,

    "globals"   : {
        // libs
        "$"                    : true,
        "window"               : true,
        "console"              : true
    }
}
